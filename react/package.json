  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    <%_ if(locals.useAntd){ _%>
    "antd": "^3.25.0",
    <%_ } _%>
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    <%_ if(locals.store === 'mobx'){ _%>
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    <%_ } _%>
    "react": "^16.11.0",
    "react-app-rewired": "^2.1.4",
    "react-dom": "^16.11.0",
    <%_ if(locals.store === 'redux'){ _%>
    "react-redux": "^7.1.1",
    <%_ } _%>
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    <%_ if(locals.store === 'redux'){ _%>
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    <%_ } _%>
    <%_ if(locals.store === 'redux' && locals.reduxPlugin === 'redux-saga'){ _%>
    "redux-saga": "^1.1.3",
    <%_ } else { _%>
    "redux-thunk": "^2.3.0"
    <%_ } _%>
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    <%_ if(locals.useAntd) { _%>
    "babel-plugin-import": "^1.12.2",
    <%_ } _%>
    "customize-cra": "^0.8.0",
    "http-proxy-middleware": "^0.20.0",
    <%_ if(locals.preprocessor === 'less'){ _%>
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    <%_ } else { _%>
    "node-sass": "^4.13.0",
    <%_ } _%> 
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
